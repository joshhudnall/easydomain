{
    "name": "joshhudnall/easy-domain",
    "version": "1.0.3",
    "description": "A CLI command to easily create domains and stubs.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Josh Hudnall",
            "email": "josh@wearesupermagical.com",
            "homepage": "https://joshhudnall.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoshHudnall\\EasyDomain\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^v11.13.0|^v10.48.14|^9.2",
        "laravel/framework": "^v11.13.0|^v10.48.14|^9.2"
    },
    "require-disabled": {
        "laravel/prompts": "^0.1.24"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoshHudnall\\EasyDomain\\Providers\\EasyDomainServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
